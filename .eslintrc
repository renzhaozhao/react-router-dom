{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "semi": [2, "never"],
    "comma-dangle": [0],
    "react/jsx-filename-extension": [1, {
      "extensions": [".js", ".jsx"]
    }],
    "no-undef": [0],
    "react/no-unused-prop-types": [0],
    "jsx-a11y/no-static-element-interactions": [0],
    "no-console": [1],
    "react/prop-types": [0],
    "global-require": [0]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
